  "dependencies": {
    "@types/jsonwebtoken": "^9.0.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.1.2",
    "rimraf": "^4.1.2",
    "sucrase": "^3.29.0",
    "ts-jest": "^29.0.5",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
