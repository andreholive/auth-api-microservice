{
  "name": "auth-microservice",
  "version": "1.0.0",
  "description": "a auth api with JWT",
  "main": "index.ts",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "rimraf ./build && tsc && node build/index.js",
    "dev": "ts-node-dev src/index.ts",
    "test": "jest",
    "typeorm": "typeorm-ts-node-commonjs",
